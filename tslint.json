{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      180
    ],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "curly": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}